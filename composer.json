{
    "name": "vasildakov/postcode-doctrine",
    "description": "UK Postcode Doctrine Type",
    "type": "library",
    "keywords": [
        "uk",
        "postcode",
        "doctrine",
        "type",
        "php"
    ],
    "homepage": "https://github.com/vasildakov/postcode-doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "vasildakov/postcode": "^1.1",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/php-code-coverage": "~3"
    },
    "suggest": {
        "vasildakov/postcode-parser": "^1.*",
        "vasildakov/postcode-validator": "^1.*"
    },
    "autoload": {
        "psr-4": {
            "VasilDakov\\Postcode\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakovTest\\Postcode\\Doctrine\\": "tests/"
        }
    }
}
