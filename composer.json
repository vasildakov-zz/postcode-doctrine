    "require-dev": {
        "phpunit/phpunit": "~5.3",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/php-code-coverage": "~3"
    },
    "autoload": {
        "psr-4": {
